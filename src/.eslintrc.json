{
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:react/recommended"
    ],
    "plugins": ["react", "import", "prettier"],
    "rules": {
      "react/prop-types": 0,
      "no-unused-vars":"warn",
      "react/no-unescaped-entities": "off",
      "react/no-unknown-property": "off"



    },

  
    
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }